#include <stdio.h>
void expandString(const char input[]) {
    char currentChar = '\0';
    int count = 0;
    for (int i = 0; input[i] != '\0'; ++i) {
        if (isalpha(input[i])) {
            while (count > 0) {
                printf("%c", currentChar);
                --count;
            }
            currentChar = input[i];
        } else if (isdigit(input[i])) {
         count = count * 10 + (input[i] - '0');
        }
    }
    while (count > 0) {
        printf("%c", currentChar);
        --count;
    }
    printf("\n");
}
int main() {
    char input[100];
    printf("Enter the input string: ");
    scanf("%s", input);
    printf("Output: ");
    expandString(input);
    return 0;
}
