#include <stdio.h>
int main() {
    int x, y, z;
    printf("Enter three distinct numbers: ");
    scanf("%d %d %d", &x, &y, &z);
    if ((x > y && x < z) || (x < y && x > z))
        printf("1\n");
    else if ((y > x && y < z) || (y < x && y > z))
        printf("2\n");
    else
        printf("3\n");
    return 0;
}
